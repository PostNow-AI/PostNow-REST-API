{
  "builds": [
    {
      "src": "Sonora_REST_API/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.12"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/Sonora_REST_API/wsgi.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "/Sonora_REST_API/wsgi.py"
    },
    {
      "src": "/(.*)",
      "dest": "/Sonora_REST_API/wsgi.py"
    }
  ],
  "env": {
    "PYTHONPATH": ".",
    "DJANGO_SETTINGS_MODULE": "Sonora_REST_API.settings"
  },
  "functions": {
    "Sonora_REST_API/wsgi.py": {
      "maxDuration": 30
    }
  }
}
