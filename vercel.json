{
  "builds": [
    {
      "src": "Sonora_REST_API/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.12"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/Sonora_REST_API/wsgi.py"
    }
  ],
  "crons": [
    {
      "path": "/api/v1/ideabank/cron/daily-content-generation/?batch=1",
      "schedule": "0 3 * * *"
    },
    {
      "path": "/api/v1/ideabank/cron/daily-content-generation/?batch=2",
      "schedule": "5 3 * * *"
    },
    {
      "path": "/api/v1/ideabank/cron/daily-content-generation/?batch=3",
      "schedule": "10 3 * * *"
    },
    {
      "path": "/api/v1/ideabank/cron/daily-content-generation/?batch=4",
      "schedule": "15 3 * * *"
    },
    {
      "path": "/api/v1/ideabank/cron/daily-content-generation/?batch=5",
      "schedule": "20 3 * * *"
    },
    {
      "path": "/api/v1/ideabank/cron/daily-content-generation/?batch=6",
      "schedule": "25 3 * * *"
    },
    {
      "path": "/api/v1/ideabank/cron/daily-content-generation/?batch=7",
      "schedule": "30 3 * * *"
    },
    {
      "path": "/api/v1/ideabank/cron/daily-content-generation/?batch=8",
      "schedule": "35 3 * * *"
    },
    {
      "path": "/api/v1/ideabank/cron/daily-content-generation/?batch=9",
      "schedule": "40 3 * * *"
    },
    {
      "path": "/api/v1/ideabank/cron/daily-content-generation/?batch=10",
      "schedule": "45 3 * * *"
    },
    {
      "path": "/api/v1/ideabank/cron/daily-content-generation/?batch=11",
      "schedule": "50 3 * * *"
    },
    {
      "path": "/api/v1/ideabank/cron/daily-content-generation/?batch=12",
      "schedule": "55 3 * * *"
    }
  ],
  "env": {
    "PYTHONPATH": ".",
    "DJANGO_SETTINGS_MODULE": "Sonora_REST_API.settings"
  }
}
